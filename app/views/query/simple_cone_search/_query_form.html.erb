<%= form_tag "/query/conesearch", remote: true, class: 'form-horizontal', role: 'form', method: 'get' , name: 'almaSearch' do %>

<div id="actions_states" class="alert fade in alert-info" style="margin-bottom:0px">
    <strong>Heads up! </strong> This will show the states of actions
</div>
<table id="table_query" class="" >
	<thead>
		<tr>
			<th>	
				<%= button_tag 'Reset', type: 'reset', class: 'btn btn-default ', style: "display:inline-block;margin-left;" %>	
			</th>
			<th>
			</th>
			<th></th>
			<th>
				<span id="flag" class="fa-stack fa-lg" style="float:right">
				  <i class="fa fa-circle fa-stack-2x"></i>
				  <i class="fa fa-flag fa-stack-1x fa-inverse"></i>
				</span>

				<span id="info_states" class="fa-stack fa-lg" style="float:right">
				  <i class="fa fa-circle fa-stack-2x"></i>
				  <i class="fa fa-info fa-stack-1x fa-inverse"></i>
				</span>
			</th>		
			</tr>	
	</thead>
	<tbody>
		<tr>
			<td id="position" class="panel-default margin-bottom-40" style="left:390px">
				<%= render 'query/simple_cone_search/position' %>
			</td>
		</tr>
	</tbody>
</table>
<% end %>
<br>
<%= form_tag "/query/conesearch", remote: true, class: 'form-horizontal', role: 'form', method: 'post', name: 'searchForm'  do %>
	<%= button_tag(type: "submit", class: "btn btn-default", name: "commit", value:"process_position_query_list", id: 'process_position_query_list', style: "display:inline-block") do %>
	    Process
	<% end %>
	<i id="estate_process_query_list" class="" style="display:inline-block; margin-left:10px;"></i>
	<br>
	<table id= "query_list_table" class="table">
		<thead>
			<tr>
				<th>Sesame Name</th>
				<th>Right Ascension</th>
				<th>Declination</th>
				<th>Search Radius</th>
				<th>Resource</th>
				<th>Action</th>	
			</tr>
		</thead>
		<tbody>
		</tbody>
	</table>
<% end %>
 <!--=== End Form ===-->
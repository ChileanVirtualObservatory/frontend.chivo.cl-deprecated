<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(feature) -%>">
  <span class='title'>
    <%= feature.title %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.features_feature_path(feature),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_features_admin_feature_path(feature),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.features_admin_feature_path(feature),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => feature.title),
        :method => :delete %>
  </span>
</li>
